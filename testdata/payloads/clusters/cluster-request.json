{
  "kind": "Cluster",
  "name": "hp-cluster-{{.Random}}",
  "labels": {
    "environment": "production",
    "team": "platform"
  },
  "spec": {
    "platform": {
      "type": "gcp",
      "gcp": {
        "projectID": "my-gcp-project",
        "region": "us-central1",
        "zone": "us-central1-a",
        "network": "default",
        "subnet": "default-subnet"
      }
    },
    "release": {
      "image": "registry.redhat.io/openshift4/ose-cluster-version-operator:v4.14.0",
      "version": "4.14.0"
    },
    "networking": {
      "clusterNetwork": [
        {
          "cidr": "10.10.0.0/16",
          "hostPrefix": 24
        }
      ],
      "serviceNetwork": ["10.96.0.0/12"]
    },
    "dns": {
      "baseDomain": "example.com"
    }
  }
}
